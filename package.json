{
  "name": "pride-london-web",
  "version": "0.1.0",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint 'src/**/*.[jt]s?(x)'",
    "lint-fix": "eslint --fix 'src/**/*.[jt]s?(x)'",
    "test": "yarn jest --runInBand && codecov",
    "test:watch": "yarn jest --watch",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(*.test|*.stories).{ts,tsx}": "yarn tsc --skipLibCheck --strict --esModuleInterop --lib \"esnext, dom, dom.iterable\" --jsx preserve --typeRoots \"./@types, ./node_modules/@types\" --noEmit",
    "src/**/*.{js,json,ts,tsx}": [
      "eslint --fix",
      "stylelint",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.7.4",
    "@rebass/grid": "^6.0.0",
    "debounce": "^1.2.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.18.7",
    "gatsby-image": "^2.2.34",
    "gatsby-link": "^2.2.25",
    "gatsby-plugin-layout": "^1.1.16",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-rollbar": "^1.0.0",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-sitemap": "^2.2.22",
    "gatsby-plugin-styled-components": "^3.1.14",
    "gatsby-source-contentful": "^2.1.67",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-sharp": "^2.3.7",
    "js-cookie": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.23.0",
    "no-scroll": "^2.1.1",
    "polished": "^3.4.2",
    "querystring": "^0.2.0",
    "react-accessible-accordion": "^3.0.1",
    "react-dates": "16.7.0",
    "react-flip-move": "^3.0.4",
    "react-helmet": "^5.2.1",
    "react-intercom": "^1.0.15",
    "react-markdown": "^4.2.2",
    "react-onclickoutside": "^6.9.0",
    "react-slick": "^0.25.2",
    "react-test-renderer": "^16.10.1",
    "react-typeform-embed": "^0.2.1",
    "shortid": "^2.2.15",
    "slick-carousel": "^1.8.1",
    "string-hash": "^1.1.3",
    "styled-components": "^4.4.1",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@contentful/rich-text-react-renderer": "^13.2.0",
    "@types/jest": "^24.0.22",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-remove-graphql-queries": "^2.7.10",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "codecov": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gatsby-plugin-typescript": "^2.1.15",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom": "^24.0.0",
    "jest-styled-components": "^6.3.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.18.2",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.12.0",
    "stylelint": "^12.0.0",
    "stylelint-a11y": "^1.2.2",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "typescript": "^3.7.2",
    "typescript-styled-plugin": "^0.15.0"
  }
}
