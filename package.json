{
  "name": "pride-london-web",
  "version": "0.1.0",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "lint-fix": "eslint --fix 'src/**/*.js'",
    "test": "yarn jest --runInBand && codecov",
    "test:watch": "yarn jest --watch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg)$": "<rootDir>/empty-module.js"
    },
    "testMatch": [
      "**/?(*_)(spec|test).js"
    ],
    "setupFiles": [
      "./src/configuration/jest.config.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "globals": {
      "__PATH_PREFIX__": ""
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged "
    }
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "eslint"
    ]
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@rebass/grid": "^6.0.0",
    "debounce": "^1.2.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "gatsby": "^2.9.7",
    "gatsby-image": "^2.1.4",
    "gatsby-link": "^2.0.17",
    "gatsby-plugin-layout": "^1.0.15",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-rollbar": "^1.0.0",
    "gatsby-plugin-sharp": "^2.1.7",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-contentful": "^2.0.73",
    "gatsby-source-filesystem": "^2.0.41",
    "gatsby-transformer-sharp": "^2.1.21",
    "js-cookie": "^2.2.0",
    "moment": "^2.23.0",
    "no-scroll": "^2.1.1",
    "polished": "^3.4.0",
    "querystring": "^0.2.0",
    "react-accessible-accordion": "^2.4.5",
    "react-dates": "16.7.0",
    "react-flip-move": "^3.0.3",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.0.8",
    "react-onclickoutside": "^6.8.0",
    "react-slick": "^0.24.0",
    "react-test-renderer": "^16.8.6",
    "shortid": "^2.2.14",
    "slick-carousel": "^1.8.1",
    "string-hash": "^1.1.3",
    "styled-components": "^4.2.1",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@contentful/rich-text-react-renderer": "^13.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-remove-graphql-queries": "^2.6.3",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "codecov": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.4.1",
    "jest": "^23.6.0",
    "jest-environment-jsdom": "^24.8.0",
    "jest-styled-components": "^6.3.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
