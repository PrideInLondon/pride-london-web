{
  "name": "pride-london-web",
  "version": "0.1.0",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "eslintIgnore": [
    "src/**/*_spec.js"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "test": "yarn run jest && codecov",
    "test:watch": "yarn run jest --watch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg)$": "<rootDir>/empty-module.js"
    },
    "testMatch": [
      "**/?(*_)(spec|test).js"
    ],
    "setupFiles": [
      "./src/configuration/jest.config.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "globals": {
      "__PATH_PREFIX__": ""
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged "
    }
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "eslint"
    ]
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@rebass/grid": "^6.0.0-7",
    "dotenv": "^7.0.0",
    "gatsby": "^2.2.3",
    "gatsby-link": "^2.0.15",
    "gatsby-plugin-layout": "^1.0.13",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-rollbar": "^1.0.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-contentful": "^2.0.40",
    "gatsby-source-filesystem": "^2.0.27",
    "moment": "^2.23.0",
    "no-scroll": "^2.1.1",
    "polished": "^3.0.3",
    "querystring": "^0.2.0",
    "react-accessible-accordion": "^2.4.5",
    "react-dates": "16.7.0",
    "react-flip-move": "^3.0.3",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.0.6",
    "react-onclickoutside": "^6.8.0",
    "react-test-renderer": "^16.8.4",
    "shortid": "^2.2.14",
    "string-hash": "^1.1.3",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "codecov": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-environment-jsdom": "^24.5.0",
    "jest-styled-components": "^6.3.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  }
}
